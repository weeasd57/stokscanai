{
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": [
                    "models/**"
                ]
            }
        },
        {
            "src": "web/package.json",
            "use": "@vercel/next"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        }
    ]
}